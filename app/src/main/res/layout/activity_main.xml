<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">


    <RelativeLayout
        android:layout_width="983dp"
        android:layout_height="match_parent">
        <com.tencent.smtt.sdk.WebView
            android:id="@+id/x5webview"
            android:layout_width="500dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginStart="-4dp"
            android:layout_marginLeft="-4dp"
            android:layout_marginTop="0dp"
            android:layout_marginEnd="133dp"
            android:layout_marginRight="133dp">

            <RelativeLayout
                android:layout_width="1199dp"
                android:layout_height="match_parent">

                <com.example.webviewdemo.RockerViewMid
                    android:id="@+id/rockerView1"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="576dp"
                    android:layout_marginLeft="576dp"
                    android:layout_marginBottom="25dp" />

                <com.example.webviewdemo.RockerView
                    android:id="@+id/rockerView2"
                    android:layout_width="120dp"
                    android:layout_height="120dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="50dp"
                    android:layout_marginLeft="50dp"
                    android:layout_marginBottom="26dp" />


                <SeekBar
                    android:id="@+id/seekBar1"
                    style="@style/Widget.AppCompat.SeekBar.Discrete"
                    android:layout_width="100dp"
                    android:layout_height="80dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="318dp"
                    android:layout_marginLeft="318dp"
                    android:layout_marginBottom="225dp"
                    android:max="3"
                    android:progress="3"
                    android:rotation="90" />

                <SeekBar
                    android:id="@+id/seekBar2"
                    style="@style/Widget.AppCompat.SeekBar.Discrete"
                    android:layout_width="100dp"
                    android:layout_height="80dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="78dp"
                    android:layout_marginLeft="78dp"
                    android:layout_marginBottom="228dp"
                    android:max="3"
                    android:progress="3"
                    android:rotation="90" />

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="266dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="244dp"
                    android:layout_marginLeft="244dp"
                    android:layout_marginBottom="64dp"
                    android:text="TextView"
                    android:textSize="36sp" />

                <SeekBar
                    android:id="@+id/seekBar3"
                    android:layout_width="188dp"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="471dp"
                    android:layout_marginLeft="471dp"
                    android:layout_marginBottom="276dp" />

                <Switch
                    android:id="@+id/switch1"
                    android:layout_width="90dp"
                    android:layout_height="64dp"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="256dp"
                    android:layout_marginLeft="256dp"
                    android:layout_marginBottom="125dp"
                    android:text="解锁" />

                <Button
                    android:id="@+id/button1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginStart="388dp"
                    android:layout_marginLeft="388dp"
                    android:layout_marginBottom="134dp"
                    android:text="连接" />


            </RelativeLayout>
        </com.tencent.smtt.sdk.WebView>

    </RelativeLayout>


</LinearLayout>